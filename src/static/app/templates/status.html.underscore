<% if (service.get("status")) { var serviceStatus = status.statuses[service.get("status")]; %>
<i class="service-status-display fa fa-fw fa-<%- serviceStatus.fontAwesome %>" data-service="<%- service.get("slug") %>" title="Service status: <%- serviceStatus.label %>" style="color: <%- serviceStatus.color %>"> </i>
<% } %>